<!-- base template -->
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Libs CSS -->
    <link rel="stylesheet" href="/bundles/landkit/fonts/Feather/feather.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/@fancyapps/fancybox/dist/jquery.fancybox.min.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/aos/dist/aos.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/choices.js/public/assets/styles/choices.min.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/flickity-fade/flickity-fade.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/flickity/dist/flickity.min.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/highlightjs/styles/vs2015.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/jarallax/dist/jarallax.css">
    <link rel="stylesheet" href="/bundles/landkit/libs/quill/dist/quill.core.css" />

    <!-- font awesome -->
    <script src="https://kit.fontawesome.com/be1c31fb58.js" crossorigin="anonymous"></script>

    <!-- fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">

    <!-- Jquery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js" integrity="sha512-bLT0Qm9VnAYZDflyKcBaQ2gg0hSYNQrJ8RilYldYQ1FxQYoCLtUjuuRuZo+fjqhx/qtq/1itJ0C2ejDxltZVFg==" crossorigin="anonymous"></script>

    <!-- chart -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js" integrity="sha512-d9xgZrVZpmmQlfonhQUvTR7lMPtO7NkZMkA0ABN3PHCbKA5nqylQ/yWlFAyY6hYgdF1Qh6nYiuADWwKB4C2WSw==" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.bundle.min.js" integrity="sha512-SuxO9djzjML6b9w9/I07IWnLnQhgyYVSpHZx0JV97kGBfTIsUYlWflyuW4ypnvhBrslz1yJ3R+S14fdCWmSmSA==" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.css" integrity="sha512-/zs32ZEJh+/EO2N1b0PEdoA10JkdC3zJ8L5FTiQu82LR9S/rOQNfQN7U59U9BC12swNeRAz3HSzIL2vpp4fv3w==" crossorigin="anonymous" />

    <style>
        @font-face {
            font-family: "HKGroteskPro";
            font-weight: 400;
            src: url("/bundles/landkit/fonts/HKGroteskPro/HKGroteskPro-Regular.woff2") format("woff2"), url("/bundles/landkit/fonts/HKGroteskPro/HKGroteskPro-Regular.woff") format("woff"); }

        @font-face {
            font-family: "HKGroteskPro";
            font-weight: 600;
            src: url("/bundles/landkit/fonts/HKGroteskPro/HKGroteskPro-Medium.woff2") format("woff2"), url("/bundles/landkit/fonts/HKGroteskPro/HKGroteskPro-Medium.woff") format("woff"); }
    </style>

    <!-- Map -->
    <link href='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.css' rel='stylesheet' />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="/bundles/landkit/css/theme.css">

    <title>id-vault</title>

</head>
<body>
<!-- MENU -->
{% if noMenu is not defined %}
    {% include 'landkit/menu.html.twig' %}
{% endif %}

{% set flashes = [] %}
{% for label, messages in app.flashes() %}
    {% for message in messages %}
        {% if message not in flashes %}
        {% set flashes = flashes|merge([message]) %}
        <div class="alert alert-{% if label == 'error' %}{% set label = 'danger' %}{% endif %}{{ label }} rounded-0 mb-0">
            <div class="container-md">
                <div class="row">
                    <div class="col-12">
                        <!-- Text -->
                        <p class="text-center mb-0">
                            {{ message|trans|capitalize }}
                        </p>
                    </div>
                </div>
            </div>
        </div>
        {% endif %}
    {% endfor %}
{% endfor %}

<!-- Content -->
{% block content %}

{% endblock %}

<!-- FOOTER -->
{% if noFooter is not defined %}
    {% include 'landkit/footer.html.twig' %}
{% endif %}

<!-- JAVASCRIPT -->
<!-- Libs JS -->
<script src="/bundles/landkit/libs/jquery/dist/jquery.min.js"></script>
<script src="/bundles/landkit/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="/bundles/landkit/libs/@fancyapps/fancybox/dist/jquery.fancybox.min.js"></script>
<script src="/bundles/landkit/libs/aos/dist/aos.js"></script>
<script src="/bundles/landkit/libs/choices.js/public/assets/scripts/choices.min.js"></script>
<script src="/bundles/landkit/libs/countup.js/dist/countUp.min.js"></script>
<script src="/bundles/landkit/libs/dropzone/dist/min/dropzone.min.js"></script>
<script src="/bundles/landkit/libs/flickity/dist/flickity.pkgd.min.js"></script>
<script src="/bundles/landkit/libs/flickity-fade/flickity-fade.js"></script>
<script src="/bundles/landkit/libs/highlightjs/highlight.pack.min.js"></script>
<script src="/bundles/landkit/libs/imagesloaded/imagesloaded.pkgd.min.js"></script>
<script src="/bundles/landkit/libs/isotope-layout/dist/isotope.pkgd.min.js"></script>
<script src="/bundles/landkit/libs/jarallax/dist/jarallax.min.js"></script>
<script src="/bundles/landkit/libs/jarallax/dist/jarallax-video.min.js"></script>
<script src="/bundles/landkit/libs/jarallax/dist/jarallax-element.min.js"></script>
<script src="/bundles/landkit/libs/quill/dist/quill.min.js"></script>
<script src="/bundles/landkit/libs/smooth-scroll/dist/smooth-scroll.min.js"></script>
<script src="/bundles/landkit/libs/typed.js/lib/typed.min.js"></script>

<!-- Map -->
<script src='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.js'></script>

<!-- Theme JS -->
<script src="/bundles/landkit/js/theme.min.js"></script>

{% if app.user is defined and app.user is not empty and app_session_duration is defined and app_session_duration is not empty %}
    <script>
        setTimeout(function () {
            location.href = '{{ path('app_user_logout') }}';
        }, {{ (app_session_duration * 1000) + 5000 }});
    </script>
{% endif %}

</body>
</html>
