{% extends 'dashboard/dashboard.html.twig' %}

{% block pageContent %}
    <div class="container-md">
        <div class="row">
            {#            <div class="col-12 col-md-3">#}

            {#                {% set activeItem = 'claims' %}#}
            {#                {% include 'dashboard/sidenavbar.html.twig' %}#}

            {#            </div>#}

            {# TODO:Include the sidenavbar and have a back button? #}

            <div class="col-12 col-md-9">

                <!-- Card -->
                <div class="card card-bleed shadow-light-lg mb-6">
                    <div class="card-header">
                        <div class="row align-items-center">
                            <div class="col-2">

                                <!-- Back Button -->
                                <a class="btn btn-sm btn-primary" href="{{ path('app_dashboard_authorizations') }}">
                                    {{ 'back'|trans|capitalize }}
                                </a>

                            </div>

                            <div class="col-8 text-center">

                                <!-- Heading -->
                                <h4 class="mb-0">{{ 'authorization'|trans|capitalize }}</h4>
                                <h4 class="mb-0">
                                    {% if resource.application.contact is defined and resource.application.contact is not empty and commonground_is_resource(resource.application.contact) %}
                                        {% set application = commonground_resource(resource.application.contact) %}

                                        {% if application.organization.name is defined and application.organization.name is not empty %}
                                            {{ application.organization.name }} | {{ application.name }}
                                        {% else %}
                                            {{ application.name }}
                                        {% endif %}
                                    {% elseif resource.application.name is defined and resource.application.name is not empty %}
                                        {{ resource.application.name }}
                                    {% else %}
                                        {{ resource.id~' '~'this authorization has no application!'|trans|capitalize }}
                                    {% endif %}
                                </h4>

                            </div>

                            <div class="col-2">
                                <!-- Icon -->
                                <div class="avatar avatar-xl">
                                    <img class="avatar-img"
                                        {% if application.organization.style.favicon.base64 is defined and application.organization.style.favicon.base64 is not empty %}
                                            src="{{ application.organization.style.favicon.base64 }}"
                                        {% else %}
                                            src="data:image/png;base64,"
                                        {% endif %}
                                         alt="..."
                                         style="object-fit: fill;
                                         {% if resource.backgroundColor is defined and resource.backgroundColor|length > 0 %}
                                             background-color: {{ resource.backgroundColor[1] }};
                                         {% endif %}"
                                    >
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        {% if resource|default %}

                            {% if resource.scopes is defined and resource.scopes is not empty %}
                                <!-- Authorization scopes -->
                                <h4>Scopes:</h4>
                                <small class="text-gray-700">
                                    {% for scope in resource.scopes %}
                                        {{ scope }}<br>
                                    {% endfor %}
                                </small>
                                <br>
                            {% endif %}

                            {% if resource.endDate is defined and resource.endDate is not empty %}
                                <br>

                                <!-- Authorization endDate -->
                                <h4>{{ 'end date'|trans|capitalize }}:</h4>
                                <small class="text-gray-700">
                                    {{ resource.endDate|date("d-m-Y", "Europe/Paris") }}
                                </small>
                            {% endif %}

                        {% endif %}
                    </div>
                </div>

                {% if resource.dossiers is defined and resource.dossiers is not empty %}
                    <!-- Card -->
                    <div class="card card-bleed shadow-light-lg mb-6">
                        <div class="card-header">
                            <div class="row align-items-center">
                                <div class="col text-center">

                                    <!-- Heading -->
                                    <h4 class="mb-0">
                                        {{ 'this are the dossiers on this authorization:'|trans|capitalize }}
                                    </h4>

                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <!-- list of dossiers -->
                            {% set dossiers = resource.dossiers %}
                            {% include 'dashboard/widgets/dossier_list.html.twig' %}
                        </div>
                    </div>
                {% endif %}

                {% if resource.authorizationLogs is defined and resource.authorizationLogs is not empty %}
                    <!-- Card -->
                    <div class="card card-bleed shadow-light-lg mb-6">
                        <div class="card-header">
                            <div class="row align-items-center">
                                <div class="col text-center">

                                    <!-- Heading -->
                                    <h4 class="mb-0">
                                        {{ 'logs'|trans|capitalize }}
                                    </h4>

                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <!-- list of logs -->
                            {% set logs = resource.authorizationLogs %}
                            {% include 'dashboard/widgets/log_list.html.twig' %}
                        </div>
                    </div>
                {% endif %}

            </div>
        </div> <!-- / .row -->
    </div> <!-- / .container -->

    <!-- Modals -->
    {% if resource.dossiers is defined and resource.dossiers is not empty %}
        {% for dossier in resource.dossiers %}
            {% include 'dashboard/modals/delete_dossier_modal.html.twig' %}
        {% endfor %}
    {% endif %}

{% endblock %}
