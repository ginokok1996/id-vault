{% extends 'dashboard/dashboard.html.twig' %}

{% block pageContent %}
    <div class="container-md">
        <div class="row">
            <div class="col-12 col-md-3">

                {% set activeItem = 'dossiers' %}
                {% include 'dashboard/sidenavbar.html.twig' %}

            </div>

            <div class="col-12 col-md-9">

                <!-- Card -->
                <div class="card card-bleed shadow-light-lg mb-6">
                    <div class="card-header">
                        <div class="row align-items-center">
                            <div class="col-1">

                                <!-- Back Button -->
                                <a class="btn btn-sm btn-primary" href="{{ path('app_dashboard_dossiers') }}">
                                    {{ 'back'|trans|capitalize }}
                                </a>

                            </div>

                            <div class="col-10 text-center">

                                <!-- Heading -->
                                <h4 class="mb-0">{{ 'dossier'|trans|capitalize }}</h4>
                                <h4 class="mb-0">
                                    {{ resource.name }}
                                </h4>

                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        {% if resource|default %}

                            {% if resource.expiryDate is defined and resource.expiryDate is not empty %}
                                <!-- Dossier endDate -->
                                <h4>{{ 'expiry date'|trans|capitalize }}:</h4>
                                <small class="text-gray-700">
                                    {{ resource.expiryDate|date("d-m-Y", "Europe/Paris") }}
                                </small>
                            {% endif %}

                        {% endif %}
                    </div>
                </div>

                {% if resource.authorization is defined and resource.authorization is not empty %}
                    <!-- Card -->
                    <div class="card card-bleed shadow-light-lg mb-6">
                        <div class="card-header">
                            <div class="row align-items-center">
                                <div class="col text-center">

                                    <!-- Heading -->
                                    <h4 class="mb-0">
                                        {{ 'this is a dossier on this authorization:'|trans|capitalize }}
                                    </h4>

                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            {% if resource.authorization.application.contact is defined and resource.authorization.application.contact is not empty and commonground_is_resource(resource.authorization.application.contact) %}
                                {% set application = commonground_resource(resource.authorization.application.contact) %}
                                <div class="row align-items-center">
                                    <div class="col-auto">

                                        <!-- Icon -->
                                        <div class="avatar avatar-xl">
                                            <img class="avatar-img"
                                                {% if application.organization.style.favicon.base64 is defined and application.organization.style.favicon.base64 is not empty %}
                                                    src="{{ application.organization.style.favicon.base64 }}"
                                                {% else %}
                                                    src="data:image/png;base64,"
                                                {% endif %}
                                                 alt="..."
                                                 style="object-fit: fill;
                                                 {% if resource.authorization.backgroundColor is defined and resource.authorization.backgroundColor|length > 0 %}
                                                     background-color: {{ resource.authorization.backgroundColor[1] }};
                                                 {% endif %}"
                                            >
                                        </div>
                                    </div>
                                    <div class="col ml-n5">

                                        <!-- Heading -->
                                        <p class="mb-0">
                                            {% if application.organization.name is defined and application.organization.name is not empty %}
                                                {{ application.organization.name }} | {{ application.name }}
                                            {% else %}
                                                {{ application.name }}
                                            {% endif %}
                                        </p>

                                        {% if resource.authorization.scopes is defined and resource.authorization.scopes is not empty %}
                                            <!-- Text -->
                                            <small class="text-gray-700">
                                                {% for scope in resource.authorization.scopes %}
                                                    {{ scope }}<br>
                                                {% endfor %}
                                            </small>
                                            <br>
                                        {% endif %}

                                        {% if resource.authorization.endDate is defined and resource.authorization.endDate is not empty %}
                                            <!-- Text2 -->
                                            <small class="text-gray-700">
                                                {{ 'end date'|trans|capitalize }}: {{ resource.authorization.endDate|date("d-m-Y", "Europe/Paris") }}
                                            </small>
                                        {% endif %}

                                    </div>
                                    <div class="col-auto">

                                        <!-- Button -->
                                        <a class="btn btn-xs btn-primary" href="{{ path('app_dashboard_authorization', {id: resource.authorization.id}) }}">
                                            {{ 'view usage'|trans|capitalize }}
                                        </a>

                                    </div>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                {% endif %}

            </div>
        </div> <!-- / .row -->
    </div> <!-- / .container -->

{% endblock %}
